// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

  /*
  
  You are given a number A in Kilometers. Convert this into B: Meters and Centi-Metres.
  
  */
  
   var x_m = parseInt(userInput) * 1000;
   var x_cm = x_m * 100; 
   console.log(x_m);
   console.log(x_cm);

  //end-here
});
